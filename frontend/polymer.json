{
  "entrypoint": "index.html",
  "shell": "src/recipes-app.html",
  "fragments": [
    "src/recipes-list.html",
    "src/recipes-view.html",
    "src/recipes-fallback.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "name": "bundled",
      "preset": "es5-bundled"
    }
  ]
}